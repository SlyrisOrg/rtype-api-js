{
  "name": "r-type_api",
  "version": "0.0.1",
  "description": "R type game server api",
  "keywords": [
    "api",
    "game"
  ],
  "repository": {
    "type": "git",
    "url": "https://git-codecommit.eu-west-1.amazonaws.com/v1/repos/twitter.git"
  },
  "main": "src/index.js",
  "author": "Phetsinorath 'Shikanime Willian <shikalegend@gmail.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "nodemon --inspect src/index.js --exec babel-node",
    "start:prod": "pm2 start ./dist/index.js --name app-production",
    "lint": "eslint src --fix",
    "test": "jest test",
    "clear": "rimraf dist",
    "build": "npm run clear && babel src -d dist --copy-files"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.15.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-validator": "^4.2.1",
    "helmet": "^3.2.0",
    "if-env": "^1.0.0",
    "jsonwebtoken": "^8.1.0",
    "mongodb": "^2.2.33",
    "morgan": "^1.8.1",
    "pm2": "^2.6.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-minify-infinity": "^0.2.0",
    "babel-plugin-minify-mangle-names": "^0.2.0",
    "babel-plugin-minify-numeric-literals": "^0.2.0",
    "babel-plugin-minify-simplify": "^0.2.0",
    "babel-plugin-transform-inline-consecutive-adds": "^0.2.0",
    "babel-plugin-transform-minify-booleans": "^6.8.3",
    "babel-plugin-transform-property-literals": "^6.8.5",
    "babel-plugin-transform-regexp-constructors": "^0.2.0",
    "babel-plugin-transform-remove-console": "^6.8.5",
    "babel-plugin-transform-remove-debugger": "^6.8.5",
    "babel-plugin-transform-undefined-to-void": "^6.8.3",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-minify-constant-folding": "^0.2.0",
    "babel-plugin-minify-dead-code-elimination": "^0.2.0",
    "babel-plugin-minify-flip-comparisons": "^0.2.0",
    "babel-plugin-minify-guarded-expressions": "^0.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0"
  }
