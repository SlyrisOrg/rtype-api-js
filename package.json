{
  "name": "r-type_api",
  "version": "0.0.1",
  "description": "R type game server api",
  "keywords": [
    "api",
    "game"
  ],
  "repository": {
    "type": "git",
    "url": "https://git-codecommit.eu-west-1.amazonaws.com/v1/repos/twitter.git"
  },
  "main": "src/index.js",
  "author": "Phetsinorath 'Shikanime Willian <shikalegend@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "npm run launch:dev",
    "start:prod": "npm run build && npm run launch:prod",
    "launch": "if-env NODE_ENV=production && npm run launch:prod || npm run launch:dev",
    "launch:dev": "nodemon --inspect src/index.js --exec babel-node",
    "beta:launch:dev": "pm2 start --interpreter ./node_modules/.bin/babel-node ./src/index.js --watch --name app",
    "launch:prod": "node dist",
    "helper:lint": "eslint src --fix",
    "helper:test": "jest",
    "helper:clear": "rimraf dist",
    "helper:security": "snyk test",
    "build": "npm run clear && babel src -d dist --copy-files"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.15.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-validator": "^4.2.1",
    "helmet": "^3.2.0",
    "if-env": "^1.0.0",
    "jsonwebtoken": "^8.1.0",
    "mongodb": "^2.2.27",
    "mongoose": "^4.12.3",
    "morgan": "^1.8.1",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^2.6.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "jest": "^21.2.1",
    "rimraf": "^2.6.1",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0"
  }
}
